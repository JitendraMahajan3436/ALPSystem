@model REIN_MES_System.Models.MM_CBM_Parameter_Category
@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/admin/_Layout.cshtml";
}

<h3>Are you sure you want to delete this?</h3>
<div>

    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Category Name")
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Category_Name)
        </dd>
        
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-danger" />
            @Html.ActionLink("Show All", "Index", null, htmlAttributes: new { @class = "btn btn-primary" })
        </div>
    }
</div>

