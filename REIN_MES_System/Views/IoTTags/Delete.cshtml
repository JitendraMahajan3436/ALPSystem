@model REIN_MES_System.Models.RS_IoT_Tags

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/admin/_Layout.cshtml";
}

<h3>Are you sure you want to delete this?</h3>
<div>   
    <dl class="dl-horizontal">
        
        <dt>
            @Html.DisplayName("Shop Name")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RS_Shops.Shop_Name)
        </dd>
        <dt>
            @Html.DisplayName("Line Name")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RS_Lines.Line_Name)
        </dd>

        <dt>
            @Html.DisplayName("Station Name")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RS_Stations.Station_Name)
        </dd>
        <dt>
            @Html.DisplayName("Tag Name")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Tag_Name)
        </dd>


    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-primary" /> @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-primary" })
            @*<input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Back to List", "Index")*@
        </div>
    }
</div>
