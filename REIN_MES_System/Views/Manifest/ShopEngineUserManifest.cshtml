@{
    ViewBag.Title = "ShopEngineUserManifest";
    Layout = "~/Views/Shared/WithoutSideBar/_Layout.cshtml";
}

<div class="row">
    @if (ViewBag.userID == 23100023)
    {
        <h4 class="bg-dark text-center">ENGINE SHOP - ERROR PROOFING STATION</h4>
        <div class="row">
            <div class="col-md-3">
                <label>Scan Engine Srno.</label>
                <input type="text" class="form-control" />
            </div>
           
            <div class="col-md-3">
                <label>Scan SubAssembly SrNo.</label>
                <input type="text" class="form-control" />
            </div>
        </div>
    }
    else if (ViewBag.userID == 5)
    {
        <h4 class="bg-dark text-center">ENGINE SHOP MANIFEST</h4>
        <div class="row">
            <div class="col-md-3">
                <label>Scan Engine Srno.</label>
                <input type="text" class="form-control" />
            </div>
        </div>
    }
    else
    {
        <h4 class="bg-dark text-center">ENGINE SHOP MANIFEST</h4>
    }

    <div class="col-md-12 manifest-wrapper">
        <img src="~/Content/images/SOPImages/mainfesteng.jpg" />
    </div>
</div>

