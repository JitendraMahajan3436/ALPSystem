@model REIN_MES_System.Models.MM_MT_Parameter

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/admin/_Layout.cshtml";
}



@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="col-md-12">

        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.Parameter_ID)

        <div class="row">
            @Html.ValidationSummary(true)
            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.Parameter_Name, "Parameter Name", htmlAttributes: new { @class = "" })
                @Html.TextBoxFor(model => model.Parameter_Name, htmlAttributes: new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.Parameter_Name)
            </div>
        </div>
        <div class="row">
            <div class=" col-md-12">
                <div class="form-group">
                    <input type="submit" value="Save" class="btn btn-primary" />  @Html.ActionLink("Show All", "Index", null, new { @class = "btn btn-primary" })
                </div>
            </div>
        </div>
    </div>
}


