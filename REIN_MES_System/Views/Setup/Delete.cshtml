@model REIN_MES_System.Models.RS_Setup
@using REIN_MES_System.App_LocalResources;
@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/admin/_Layout.cshtml";
}

<h3>Are you sure you want to delete this?</h3>

<div>
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Plant Name")
        </dt>
        <dd>
            @Html.DisplayFor(model => model.RS_Plants.Plant_Name)
        </dd>
        <dt>
            @Html.DisplayName("Shop Name")
        </dt>
        <dd>
            @Html.DisplayFor(model => model.RS_Shops.Shop_Name)
        </dd>
        <dt>
            @Html.DisplayName("Line Name")
        </dt>
        <dd>
            @Html.DisplayFor(model => model.RS_Lines.Line_Name)
        </dd>
        <dt>
            @Html.DisplayName("Setup Name")
       </dt>
        <dd>
            @Html.DisplayFor(model => model.Setup_Name)
       </dd>
        <dt>
            @Html.DisplayName("Setup Description")
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Setup_Description)
        </dd>
   </dl>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="@ResourceGlobal.Delete" class="btn btn-primary" />
            @Html.ActionLink(ResourceGlobal.Show_All, "Index", null, new { @class = "btn btn-primary" })
        </div>
    }
</div>

