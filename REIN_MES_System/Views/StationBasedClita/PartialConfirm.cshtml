@model REIN_MES_System.Models.RS_Station_Based_Clita_Log


<script type="text/javascript">
    var form = $("#create-modal-body").closest("form");
    form.removeData('validator');
    form.removeData('unobtrusiveValidation');
    $.validator.unobtrusive.parse($("form"));
</script>

<div class="row">
    <div class="col-md-9 col-md-offset-1">
        @using (Html.BeginForm("Complete", "StationBasedClita"))
        {
            @Html.ValidationSummary(true)
            @Html.HiddenFor(model => model.Station_Clita_Log_ID);

            <div class="row">
                <div class="col-md-12 ">
                    @Html.LabelFor(model => model.Station_ID, "Station Name")
                    @Html.TextBoxFor(model => model.RS_Stations.Station_Name, htmlAttributes: new { @class = "form-control", @disabled = "disabled" })
                </div>
            </div>
            
            <div class="row ">
                <div class="col-md-12">
                    @Html.LabelFor(model => model.Remark, "Remark")
                    @Html.TextBoxFor(model => model.Remark, htmlAttributes: new { @class = "form-control" })
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    @Html.LabelFor(model => model.Special_Observation, "Special Observation")
                    @Html.TextBoxFor(model => model.Special_Observation, htmlAttributes: new { @class = "form-control" })
                </div>
            </div>
            <br />
            <br />
            <div class="row">
                <button type="submit" class="btn btn-primary">Submit</button>
                <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
            </div>

        }
    </div>
</div>